{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off",
        "aria-required-children": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select",
          "border: 1px solid color-mix(in srgb, var(--mat-sys-tertiary) 35%, transparent)",
          "border-bottom: 1px solid color-mix(in srgb, var(--mat-sys-outline-variant) 60%, transparent)",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none",
          "border-color: color-mix(in srgb, var(--mat-sys-outline-variant) 55%, transparent)",
          "border-color: color-mix(in srgb, var(--mat-sys-primary) 28%, var(--mat-sys-outline-variant) 72%)",
          "border: 1px solid color-mix(in srgb, var(--mat-sys-outline-variant) 55%, transparent)",
          "border-color: color-mix(in srgb, var(--mat-sys-primary) 26%, var(--mat-sys-outline-variant) 74%)",
          "border-color: color-mix(in srgb, var(--mat-sys-error) 26%, var(--mat-sys-outline-variant) 74%)",
          "border-color: color-mix(in srgb, var(--mat-sys-error) 34%, var(--mat-sys-outline-variant) 66%)",
          "border: 1px dashed color-mix(in srgb, var(--mat-sys-outline) 50%, transparent)",
          "border: 1px solid color-mix(in srgb, var(--mat-sys-outline-variant) 58%, transparent)"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}